;;;;;;;;;;;
;; pairs ;;
;;;;;;;;;;;
(cons 1 2) ;Value 11: (1 . 2)

(cons 3 (cons 1 2)) ; (3 1 . 2)

(cons #\a (cons 3 "hello")) ; (#\a 3 . "hello")

(cons (cons 0 1) (cons 2 3)) ; ((0 . 1) 2 . 3)

;;;;;;;;;;;
;; quote ;;
;;;;;;;;;;;

'(+ 2 3) ; =>  (+ 2 3)
'+ ; => + 
'() ; => () 

;;;;;;;;;;;;;;;;
;; car & cdr  ;;
;;;;;;;;;;;;;;;;

(car '(1 2 3 4)) ; 1

(cdr '(1 2 3 4)) ; (2 3 4)

;;;;;;;;;;;
;; list  ;;
;;;;;;;;;;;
(list) ; ()

(list 1) ; (1)

(list '(1 2) '(3 4)) ; ((1 2) (3 4))

(list 0) ; (0)

(list 1 2) ; (1 2) 
